/* Modern Minimalist PySide6 QCalendar Stylesheet */

QCalendarWidget {
    background-color: white;
    border: none;
}

/* Header row with navigation arrows and month/year */
QCalendarWidget QWidget#qt_calendar_navigationbar {
    background-color: #4a90e2;
    padding: 4px;
}

/* Month/Year label in header */
QCalendarWidget QToolButton {
    color: white;
    background-color: transparent;
    font-size: 14px;
    font-weight: bold;
    padding: 5px 10px;
    border-radius: 4px;
}

/* Navigation arrows */
QCalendarWidget QToolButton#qt_calendar_prevmonth,
QCalendarWidget QToolButton#qt_calendar_nextmonth {
    color: white;
    background-color: transparent;
    qproperty-icon: none;
    min-width: 24px;
    max-width: 24px;
    min-height: 24px;
    max-height: 24px;
    border-radius: 12px;
}

/* Arrow hover states */
QCalendarWidget QToolButton#qt_calendar_prevmonth:hover,
QCalendarWidget QToolButton#qt_calendar_nextmonth:hover {
    background-color: rgba(255, 255, 255, 0.2);
}

/* Previous month arrow */
QCalendarWidget QToolButton#qt_calendar_prevmonth {
    qproperty-text: &quot;&lt;&quot;;
    font-weight: bold;
}

/* Next month arrow */
QCalendarWidget QToolButton#qt_calendar_nextmonth {
    qproperty-text: &quot;&gt;&quot;;
    font-weight: bold;
}

/* Day of week header */
QCalendarWidget QAbstractItemView::section {
    color: #666666;
    background-color: #f5f5f5;
    padding: 6px;
    font-size: 12px;
    font-weight: 500;
    border: none;
}

/* Weekend headers */
QCalendarWidget QAbstractItemView::section:first,
QCalendarWidget QAbstractItemView::section:last {
    color: #e74c3c;
}

/* Days grid */
QCalendarWidget QAbstractItemView {
    selection-background-color: #4a90e2;
    selection-color: white;
    alternate-background-color: transparent;
    background-color: white;
    outline: none;
    border: none;
}

/* Calendar cells */
QCalendarWidget QTableView {
    border: none;
    font-size: 13px;
}

/* Actual day cells */
QCalendarWidget QTableView QTableCornerButton::section,
QCalendarWidget QTableView QAbstractItemView::item {
    border: 1px solid #f3f3f3;
    min-height: 32px;
    padding: 2px;
}

/* Today's date */
QCalendarWidget QTableView QAbstractItemView::item:selected {
    background-color: #4a90e2;
    color: white;
    border-radius: 2px;
}

/* Hover effect on days */
QCalendarWidget QTableView QAbstractItemView::item:hover {
    background-color: #f0f0f0;
    border-radius: 2px;
}

/* Days from other months */
QCalendarWidget QTableView QAbstractItemView::item:!enabled {
    color: #cccccc;
    background-color: #fafafa;
}

/* Weekend days */
QCalendarWidget QTableView QAbstractItemView::item[weekend=&quot;true&quot;] {
    color: #e74c3c;
}
