QCalendarWidget QAbstractItemView::section:last {
    color: #e74c3c;
}

/* Days grid */
QCalendarWidget QAbstractItemView {
    selection-background-color: #4a90e2;
    selection-color: white;
    alternate-background-color: transparent;
    background-color: white;
    outline: none;
    border: none;
}

/* Calendar cells */
QCalendarWidget QTableView {
    border: none;
    font-size: 13px;
}

/* Actual day cells */
QCalendarWidget QTableView QTableCornerButton::section,
QCalendarWidget QTableView QAbstractItemView::item {
    border: 1px solid #f3f3f3;
    min-height: 32px;
    padding: 2px;
}

/* Today's date */
QCalendarWidget QTableView QAbstractItemView::item:selected {
    background-color: #4a90e2;
    color: white;
    border-radius: 2px;
}

/* Hover effect on days */
QCalendarWidget QTableView QAbstractItemView::item:hover {
    background-color: #f0f0f0;
    border-radius: 2px;
}

/* Days from other months */
QCalendarWidget QTableView QAbstractItemView::item:!enabled {
